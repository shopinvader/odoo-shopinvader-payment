<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="shopinvader_backend_view_form" model="ir.ui.view">
        <field name="model">shopinvader.backend</field>
        <field
            name="inherit_id"
            ref="shopinvader_restapi.shopinvader_backend_view_form"
        />
        <field name="arch" type="xml">
            <page name="sale" position="inside">
                <group name="payment_method" string="Payment Method">
                    <field name="payment_method_ids" nolabel="1" />
                </group>
            </page>
        </field>
    </record>

</odoo>
