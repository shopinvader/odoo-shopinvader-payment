<?xml version="1.0" encoding="UTF-8"?>
<odoo>

<record id="acquirer_form_paypal" model="ir.ui.view">
    <field name="model">payment.acquirer</field>
    <field name="inherit_id" ref="payment_paypal.acquirer_form_paypal"/>
    <field name="arch" type="xml">
        <field name="paypal_seller_account" position="after">
            <field name="paypal_client_id"/>
            <field name="paypal_secret"/>
        </field>
    </field>
</record>

</odoo>
